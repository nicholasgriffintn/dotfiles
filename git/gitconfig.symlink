[user]
  name = Nicholas Griffin
  email = inbox@nicholasgriffin.dev
[alias]
  co = checkout
[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[core]
  excludesfile = ~/.gitignore
  editor = vscode
[apply]
  whitespace = nowarn
[mergetool]
  keepBackup = false
[difftool]
  prompt = false
[help]
  autocorrect = 1
[push]
  default = simple
[credential]
	helper = osxkeychain